#include <stdio.h>
#include <math.h>

int main(){
  float x, y , y1, y2;
  printf("Введите x:\n ");
  scanf("%f", &x);
  printf("Введите y:\n ");
  scanf("%f", &y);
  y1 = (2 * x + 4);
  y2 = ((-2) * x + 4);
  if (x <= 2 && x >= -2 && y == 0)
    printf("Hit \n");
      
  else if (x <= 1 && x >= -1 && y == 2)
    printf("Hit \n");
      
  else if (((y1 > 0 && y1 < 2) || (y2 > 0 && y2 < 2)) && (y > 0 && y < 2) && (x > -2 && x < 2))
    printf("Hit \n");
      
  else
    printf("Miss \n");
  return(0);
}
